{
	"restartable": "rs",
	"ignore": [".git", "node_modules/**/node_modules", "dist", "coverage", "logs", "*.test.js", "*.spec.js", "uploads/*"],
	"verbose": true,
	"execMap": {
		"js": "node",
		"ts": "ts-node"
	},
	"watch": ["src/", "config/"],
	"env": {
		"NODE_ENV": "development"
	},
	"ext": "js,json,ts",
	"delay": "1000"
}
